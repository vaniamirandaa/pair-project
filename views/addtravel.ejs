<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

    <form action="/schedules/add" method="post">
        <label for="travelagent" class="form-label">Category</label>
                <select name="TravelAgentId" id="TravelAgentId">  
                    <option disabled selected>--Select Travel--</option>
                    <% travelagents.forEach((e) => { %>
                        <option value="<%= e.id %>"><%= e.travel %></option>
                        <%  }) %>
                </select>
<br>
        <label for="origin">Origin</label><br>
        <input type="text" id="origin" name="origin"><br><br>

        <label for="destination">Destination</label><br>
        <input type="text" id="destination" name="destination"><br><br>

        <label for="departure">Departure</label><br>
        <input type="date" id="departure" name="departure"><br><br>

        <input type="submit" value="Submit">

      </form>
    
</body>
</html>